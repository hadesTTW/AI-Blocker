{
    "manifest_version": 3,
    "name": "AI Blocker",
    "version": "1.0",
    "description": "Blocks all AI content on the internet.",
    "permissions": ["storage"],
    "host_permissions": ["https://exordium.breadcraft.me/*"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "85ed0016-acf5-4b5b-813e-ddfc1733bee0"
      }
    }
}
  